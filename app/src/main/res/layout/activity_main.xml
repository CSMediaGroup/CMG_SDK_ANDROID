<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/video_c9"
        android:visibility="gone"
        android:padding="50dp"
        android:text="视频详情页面" />

    <TextView
        android:id="@+id/set_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/video_c9"
        android:visibility="gone"
        android:padding="30dp"
        android:text="获取token值"></TextView>

    <EditText
        android:id="@+id/panelid"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:hint="panelid"
        android:textColor="#ffffff"
        android:textColorHint="#ffffff">

    </EditText>

    <EditText
        android:id="@+id/contentid"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:hint="contentid"
        android:textColor="#ffffff"
        android:textColorHint="#ffffff">

    </EditText>

    <TextView
        android:id="@+id/fxsys"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@color/video_c9"
        android:gravity="center"
        android:text="分享四要素"
        android:textColor="#ffffff"
        android:textColorHint="#ffffff"
        android:visibility="gone">

    </TextView>

    <TextView
        android:id="@+id/class_list"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@color/video_c9"
        android:gravity="center"
        android:text="合集列表"
        android:textColor="#ffffff"
        android:textColorHint="#ffffff"
        android:visibility="gone">

    </TextView>

    <TextView
        android:id="@+id/login_activity"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@color/video_c9"
        android:gravity="center"
        android:text="登录页面"
        android:textColor="#ffffff"
        android:textColorHint="#ffffff"
        android:visibility="gone">

    </TextView>

    <TextView
        android:id="@+id/others_home_page"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@color/video_c9"
        android:gravity="center"
        android:text="SDK首页"
        android:textColor="#ffffff"
        android:textColorHint="#ffffff">

    </TextView>

    <TextView
        android:id="@+id/clear_user_info"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@color/video_c9"
        android:gravity="center"
        android:text="清除用户信息"
        android:textColor="#ffffff"
        android:textColorHint="#ffffff"></TextView>

    <Switch
        android:id="@+id/isRealease"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:text="切换正式环境"></Switch>

    <TextView
        android:id="@+id/uuid"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@color/video_c9"
        android:gravity="center"
        android:text=""
        android:enabled="true"
        android:textIsSelectable="true"
        android:focusable="true"
        android:longClickable="true"
        android:textColor="#ffffff"
        android:textColorHint="#ffffff"></TextView>

    <TextView
        android:id="@+id/test"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:text="测试页面"
        android:background="@color/video_c9"
        android:gravity="center">

    </TextView>
</LinearLayout>